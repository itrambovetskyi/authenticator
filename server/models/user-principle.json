{
  "name": "UserPrinciple",
  "base": "User",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "emailVerificationRequired": false,
  "properties": {
    "id": {
      "type": "number",
      "id": true,
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "JwtToken",
      "polymorphic": {
        "selector": "principle"
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}
